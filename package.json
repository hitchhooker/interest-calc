		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@unocss/core": "^0.37.3",
		"@unocss/preset-icons": "^0.37.3",
